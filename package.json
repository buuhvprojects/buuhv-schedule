{
    "name": "@buuhv/schedule",
    "version": "1.0.6",
    "private": false,
    "babel": {
        "presets": [
            "@babel/preset-env"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ]
    },
    "scripts": {
        "watch": "nodemon src/index.ts",
        "examples": "nodemon examples/register.ts",
        "build": "tsc --build",
        "login": "npm login --registry=https://registry.npmjs.org/"
    },
    "devDependencies": {
        "@babel/cli": "^7.5.5",
        "@babel/core": "^7.8.4",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/preset-env": "^7.8.4",
        "@babel/preset-react": "^7.0.0",
        "@types/crypto-js": "^4.0.1",
        "@types/mocha": "^8.2.2",
        "@types/node": "^14.14.37",
        "babel-core": "^6.26.3",
        "babel-loader": "^8.0.6",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "nodemon": "^2.0.6",
        "ts-node": "^9.1.1",
        "tsc-watch": "^4.2.9",
        "typescript": "^4.0.5"
    },
    "files": [
        "dist/src/**/*.js",
        "dist/src/**/*.js.map",
        "dist/src/**/*.d.ts",
        "index.js",
        "index.d.ts",
        "README.md"
    ],
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/geeknection/buuhv-schedule.git"
    },
    "keywords": [
        "schedule",
        "calendar",
        "nodejs scheduling"
    ],
    "author": "BuuhV",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/geeknection/buuhv-schedule/issues"
    },
    "publishConfig": {
        "registry": "https://registry.npmjs.org/",
        "access": "public"
    },
    "dependencies": {
        "mysql2": "^2.2.5"
    }
}
